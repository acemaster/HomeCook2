<div class='col-xs-12'>
  <div class='col-xs-2'>
  </div>
  <div class='col-xs-8'>
    <div class='hash-above col-xs-12'>
      <%= link_to 'Checkout', new_order_path, class: 'btn btn-md btn-success header right' %>
      <%= link_to 'My Cart', '#', class: 'btn btn-md btn-warning cart-icon header right' %>
    </div>
    <div class='has-above col-xs-12'>
      <div class='header'>
        <%= @chef.name %>
      </div>
      <ul class='list-group'>
      <% @chef.menus.each do |menu| %>
        <li class='list-group-item has-above'>
          <%= menu.name %>
          <button class='glyphicon glyphicon-plus btn js-btn-minus rmvBtn right has-left block move-top'> </button>
          <button class='glyphicon glyphicon-minus right btn js-btn-minus rmvBtn block move-top'></button>
        </li>
      <% end %>
      </ul>
    </div>
  </div>
  <div class='col-xs-2'>
  </div>
</div>